                        </a>
                        <a href="/my-profile">
                            <li>
                                <i class="fa-regular fa-user-vneck"></i> My Profile
                            </li>
                        </a>
                        <a href="purchase-history.html">
                            <li>
                                <i class="fa-regular fa-wallet"></i> Purchases
                            </li>
                        </a>
                        <a href="coin-packs.html">
                            <li class="coins-spl-menu">
                                <i class="fa-regular fa-coins"></i> Buy Coins
                            </li>
                        </a>
                        <a href="dguide.html">
                            <li>
                                <i class="fa-regular fa-circle-info"></i> Dashboard Guide
                            </li>
                        </a>

                        <div style="text-decoration: underline;margin-top: 15px;"><a style="color: #f14848;"
                                href="report.html"><b>Report Issues</b></a></div>
                    </ul>
                </div>
            </div>
            <script>
                //Expande Menu 1
                $('.myp-exp').click(function () {
                    $('.mypr-exp').toggleClass('downed');
                    $('.mypr-expicon').toggleClass('downed');
                })
            </script>
        </div>
        <!-- NavBar ends -->


        <div class="contents">
            <div class="all">
                <div class="dash-header">
                    <div style="width:3em;height:3em;margin: auto 5px auto 1em;border-radius: 50px;"><img
                            style="width: 3em;height:3em" src="https://cdn-icons-png.flaticon.com/512/149/149071.png">
                    </div>
                    <div style="font-size: 1.5em; margin: auto 0; font-weight: 500"><%=name%> <span
                            style="color: #808080">(Tutor)</span></div>
                    <div style="font-size:1.2em;font-weight:400;margin: auto 1em auto auto"><b><%=coins%></b> Coins available
                        <a href="coin-packs.html">
                            <button
                                style=" padding: 0.3em 0.5em; background-color: #ffff00; border: 1px solid #000;margin-left: 0.8em;cursor: pointer">Buy
                                Coins
                            </button></a>
                    </div>
                </div>
                <div class="home-contents">
                    <div class="my-courses">
                        <p style="text-align: center;font-size: 20px;font-weight: 500;margin-top: 20px;">My Registered
                            Courses</p>
                        <a href="/addcourse"><button class="addcrbtn"><i class="fa-solid fa-plus"></i> Add New Course</button></a>
                        <div class="my-courses-table">

                            <table class="tabletagstud">
                                <tr>
                                    <th title="Course Number">CR-NO</th>
                                    <th title="Course Name">Course Name</th>
                                    <th title="Student Count">Course Desc</th>
                                    <th title="Action">Action</th>
                                </tr>

                                <tr>
                                    <!-- <td>1</td>
                                    <td>Web Development</td>
                                    <td>2</td>
                                    <td>
                                        
                                    </td> -->
                                </tr>

                                <% for(var i=0;i < Object.keys(obj).length; i++ ) { %>
                                    <tr>
                                        <td>
                                            <%= i + 1 %>
                                        </td>
                                        <td>
                                            <%= obj[i].c_name %>
                                        </td>
                                        <td>
                                            <%= obj[i].c_desc %>
                                        </td>
                                        <td>
                                            <form action="/manage_course" method="post">
                                                <input type="hidden" value="<%=obj[i].c_name%>" name="c_manage">
                                                <button class="manage-stud" type="submit">Manage</button>
                                            </form>
                                        </td>
                                    </tr>
                                    <% } %>

                                <!-- <tr>
                                    <td>2</td>
                                    <td>Advanced MERN</td>
                                    <td>1</td>
                                    <td>
                                        <button class="manage-stud"><a href="/manage_course">Manage</a></button>
                                    </td>
                                </tr> -->

                                <!-- <tr>
                                    <td>3</td>
                                    <td>Development with Python </td>
                                    <td>0</td>
                                    <td>
                                        <button class="manage-stud"><a href="/manage_course">Manage</a></button>
                                    </td>
                                </tr> -->

                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</body>

</html>
<body>
    <div class="main-container">
        <!-- NavBar Starts -->
        <div class="sidenav">
            <div class="nav-menu">
                <!-- Nav Header -->
                <div class="nav-header">
                    <div class="nav-logo">
                        <img src="../../public/images/favicon/favicon-96x96.png" />
                        <div class="nav-logo-title">Skill-Lancer</div>
                    </div> <br />
                    <hr />
                </div>

                <!-- Nav-Links -->
                <div class="nav-links">
                    <ul class="listed-menu">
                        <li class="nl-mlarge curact">
                            <span class="myp-exp">
                                <i class="fa-regular fa-house"></i> Dashboard
                                <span class="angle-dwn-lister"><i
                                        class="fa-regular fa-angle-right mypr-expicon"></i></span>
                            </span>
                            <ul class="sub-list-student mypr-exp downed">
                                <a href="/interested">
                                    <li><i class="fa-regular fa-circle-dot"></i>Interested</li>
                                </a>
                                <a href="/in_enquires">
                                    <li><i class="fa-regular fa-circle-dot"></i>Personal Enquires</li>
                                </a>
                                <a href="/unlocked">
                                    <li><i class="fa-regular fa-circle-dot"></i>Unlocked Details</li>
                                </a>
                            </ul>
                        </li>

                        <a href="/my_students">
                            <li>
                                <i class="fa-regular fa-user-group"></i> My Students
                            </li>
                        </a>
